           memoryStoreEvictionPolicy="LFU" />

    <cache name="rspStoreList" maxElementsInMemory="100000"
           maxElementsOnDisk="200000"
           eternal="false"
           overflowToDisk="true"
           diskSpoolBufferSizeMB="20"
           timeToIdleSeconds="300"
